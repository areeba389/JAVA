import java.util.Scanner;

class Library{

 public static void main(String[]arg){
 
 double fine;
 System.out.printlin("Enter number of days");
 Scanner sc=new Scanner(System.in);
 int days=scanner.nextInt();
 
 
 if (days>0&&days<=5){
 
    fine=0.50*days;
  
  
  }
     
 if (days>=6&&days<=10){
 
    fine=1*days;
  
  
  }
  
  
 if (days>=10){
 
    fine=5*days;
  
  
  }
  
  
 if (days>30){
 
  System.out.printlin("membership is expired");
  
  }
    }
      }
